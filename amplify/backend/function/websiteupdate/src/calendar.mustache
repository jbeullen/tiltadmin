<!-- Info -->
<a  name="clubinfo"></a>
<div class="content-section-b">

    <div class="container">
        <div class="row">
            <hr class="section-heading-spacer">
            <div class="clearfix"></div>
            <h2 class="section-heading">Kalender 2022</h2>
            <!-- Corona Stuff -->
            <!--
            <h2 class="section-heading" style="color: #ac2925">Tot nader orde zijn alle ritten geschorst, door de maatregelen rond het Corona virus.</h2>
            -->
            <!-- Corona Stuff End -->

            <p class="lead">
            <table class="table text-left">
                <thead>
                <tr>
                    <th>Datum</th>
                    <th>Type</th>
                    <th>Rit</th>
                    <th>KM</th>
                    <th>HMS</th>
                    <th>Uur - Punten</th>
                </tr>
                </thead>
                <tbody>
                    {{#rides}}
                        <tr class="{{{style}}}"><td>{{{day}}}</td><td>{{{type}}}</td><td>{{{name}}}<br/>{{{location}}}</td><td>{{{distance}}}</td><td>{{{elevation_gain}}}</td><td>{{{time}}}<br/>{{{points}}}</td></tr>
                    {{/rides}}
                </tbody>
            </table>
            <a href="#" onclick="showArchive()" class="btn btn-default btn-lg calendar">Archief</a>
            <a href="#" onclick="showCalendar()" class="btn btn-default btn-lg archive hidden">Kalender</a>
            </p>
        </div>

    </div>

</div>

<!-- Banner -->
<div class="banner">

    <div class="container">

        <div class="row">
            <div class="col-lg-6">
